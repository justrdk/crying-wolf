{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "meteor": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-extra-strict": [0],
    "strict": [0],
    "curly": [0],
    "eqeqeq": 2,
    "no-loop-func": 2,
    "camelcase": 2,
    "semi": 2
  },
  "globals": {
    // Collections
    "Weeks": true
  }
}
